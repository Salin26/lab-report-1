#include<stdio.h>
main()
{
    int angle1,angle2,angle3;
    printf ("enter the angle's :");
    scanf("%d%d%d",&angle1,&angle2,&angle3);
    if((angle1+angle2+angle3==180)&&(angle1>0 && angle2>0 && angle3>0 ))
    {
        printf("valid");
    }
   else{
    printf("invalid");

   }
}
